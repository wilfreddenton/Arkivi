{{ define "title" }}<title>Arkivi | upload</title>{{ end }}
{{ define "content" }}
  <div id="dropzone">
    <input id="dropzone-input" type="file" accept=".gif, .jpg, .jpeg, .png, image/gif, image/jpg, image/jpeg, image/png" multiple>
    <span id="dropzone-icon" class="glyphicon glyphicon-record"></span>
  </div>
  <div id="placeholder"><span>Drop images above to upload.</div>
  <ul id="previews">
  </ul>
{{ end }}
{{ define "scripts" }}
  <script src="static/js/rsvp.min.js"></script>
  <script src="static/js/upload.js"></script>
{{ end }}